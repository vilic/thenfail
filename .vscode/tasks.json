{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "args": ["run"],
    "tasks": [
        {
            "taskName": "incremental-build",
            "isBuildCommand": true,
            "isWatching": true,
            "problemMatcher": "$tsc-watch"
